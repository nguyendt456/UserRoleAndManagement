<div class="body__wrapper">
            <div class="body__userlist__wrapper">
                <table class="body__userlist">
                    <caption class="body__userlist__caption">
                        User List
                    </caption>
                    <thead>
                        <tr classname="body__userlist__head__row">
                            <th class="body__userlist__head__row-1 row-id">
                                ID
                            </th>
                            <th class="body__userlist__head__row-2 row-user">
                                Username
                            </th>
                            <th class="body__userlist__head__row-3 row-email">
                                Email
                            </th>
                            <th class="body__userlist__head__row-4 row-role">
                                Role
                            </th>
                            <th class="body__userlist__head__row-5 row-action">
                                Action
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for user in users %}
                        <tr classname="body__userlist__body__row">
                            <td class="body__userlist__head__row-1 row-id">
                                {{user.id}}
                            </td>
                            <td class="body__userlist__head__row-2 row-user">
                                {{user.username}}
                            </td>
                            <td class="body__userlist__head__row-3 row-email">
                                {{user.email}}
                            </td>
                            <td class="body__userlist__head__row-4 row-role">
                                {{user.role[0].name}}
                            </td>
                            <td class="body__userlist__head__row-5 row-action">
                                <button
                                    class="body__userlist__button-delete ripple"
                                >
                                    <span class="material-icons">
                                        delete
                                    </span>
                                    Delete
                                </button>
                                <button
                                    class="body__userlist__button-modify ripple"
                                >
                                    <span class="material-icons">
                                        edit
                                    </span>
                                    Modify
                                </button>
                            </td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>